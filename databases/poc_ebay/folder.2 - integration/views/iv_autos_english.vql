CREATE OR REPLACE VIEW iv_autos_english FOLDER = '/2 - integration' AS SELECT DISTINCT bv_autos.datecrawled AS datecrawled, bv_autos.name AS name, replacemap(bv_autos.seller, 'seller_all_eng') AS seller_eng, replacemap(bv_autos.offertype, 'offertype_all_eng') AS offertype_eng, bv_autos.price AS price, bv_autos.abtest AS abtest, bv_autos.vehicletype AS vehicletype, bv_autos.yearofregistration AS yearofregistration, replacemap(bv_autos.gearbox, 'gearbox_all_eng') AS gearbox_eng, bv_autos.powerps AS powerps, bv_autos.model AS model, bv_autos.kilometer AS kilometer, bv_autos.monthofregistration AS monthofregistration, bv_autos.fueltype AS fueltype, bv_autos.brand AS brand, replacemap(bv_autos.notrepaireddamage, 'notrepaireddamage_all_eng') AS notrepaireddamage_eng, bv_autos.datecreated AS datecreated, bv_autos.nrofpictures AS nrofpictures, bv_autos.postalcode AS postalcode, bv_autos.lastseen AS lastseenonline FROM bv_autos;

ALTER VIEW iv_autos_english
 LAYOUT (bv_autos = [20, 20, 200, 200]);

