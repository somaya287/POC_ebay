CREATE OR REPLACE VIEW iv_autos_eng_significantdata FOLDER = '/2 - integration' DESCRIPTION = 'Cleaning ; remover rows having insignificant yearofregistration, powerPS and price '  AS SELECT iv_autos_eng.datecrawled AS datecrawled, iv_autos_eng.name AS name, iv_autos_eng.seller_eng AS seller_eng, iv_autos_eng.offertype_eng AS offertype_eng, iv_autos_eng.price_decimal AS price_decimal, iv_autos_eng.abtest AS abtest, iv_autos_eng.vehicletype AS vehicletype, iv_autos_eng.yearofregistration AS yearofregistration, iv_autos_eng.gearbox_eng AS gearbox_eng, iv_autos_eng.powerps_nb AS powerps_nb, iv_autos_eng.model AS model, iv_autos_eng.kilometer AS kilometer, iv_autos_eng.monthofregistration AS monthofregistration, iv_autos_eng.fueltype AS fueltype, iv_autos_eng.brand AS brand, iv_autos_eng.notrepaireddamage_eng AS notrepaireddamage_eng, iv_autos_eng.datecreated AS datecreated, iv_autos_eng.nrofpictures AS nrofpictures, iv_autos_eng.postalcode AS postalcode, iv_autos_eng.lastseenonline AS lastseenonline FROM iv_autos_eng WHERE (yearofregistration >= 1863 AND yearofregistration <= 2022 AND powerps_nb > 0 AND powerps_nb < 1000 AND price_decimal < 100000);

ALTER VIEW iv_autos_eng_significantdata
 LAYOUT (iv_autos_eng = [69, 65, 200, 200]);

